{
  "google": {
    "clientId": "YOUR_GOOGLE_CLIENT_ID",
    "clientSecret": "PLACEHOLDER_GOOGLE_CLIENT_SECRET",
    "redirectUri": "https://newtifi.com/auth/google/callback",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.profile",
      "https://www.googleapis.com/auth/userinfo.email"
    ]
  },
  "linkedin": {
    "clientId": "YOUR_LINKEDIN_CLIENT_ID",
    "clientSecret": "PLACEHOLDER_LINKEDIN_CLIENT_SECRET",
    "redirectUri": "https://newtifi.com/auth/linkedin/callback",
      "scopes": [
        "openid",
        "profile", 
        "email"
      ]
  },
  "session": {
    "timeout": 604800000,
    "refreshThreshold": 300000,
    "storageKey": "auth_session"
  },
  "api": {
    "baseUrl": "https://api.newtifi.org",
    "timeout": 30000,
    "retryAttempts": 3
  }
}
